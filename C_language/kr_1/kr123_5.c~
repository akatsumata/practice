#include <stdio.h>

#define MAXLINE 1000

int l_getline(char line[], int maxline);


int main()
{
	int i, flag, len;
	char line[MAXLINE];

	i = flag = 0;
	while((len = l_getline(line, MAXLINE)) > 0){
		while(line[i] != '\0'){
			while(line[i] == '/'){
				++i;
				if(line[i + 1] == '*'){
					flag = 1;
					i = i + 2;
				} else {
					flag = 0;
				}
				if(flag == 1){
					if(line[i] == '*'){
						++i;
						if(line[i + 1] == '/'){
							flag = 1;
							i = i + 2;
						} else {
							flag = 0;
						}
					}
				}
			}
			if(flag == 0){
				printf("%s", line[i]);
			}
		}
		line[i] = '\0';
	}
	return 0;
}


int l_getline(char s[], int lim)
{
	int c, i;
	for(i = 0; i < lim - 1 && (c = getchar()) != EOF && c != '\n'; ++i)
		s[i] = c;
	if(c == '\n'){
		s[i] = c;
		++i;
	}
	s[i] = '\0';
	return i;
}
