void squeeze(char line[], char remove_char[])
{
        int i, j, k;
        for(i = k = 0; line[i] != '\0'; i++){
        	if(line[i] != remove_line[j]){
                	line[k++] = line[i];
                } else if(line[i] == remove_line[j]){
                        k++;
                }
        }
        s1[k] = '\0';
}

